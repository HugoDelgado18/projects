<p id="notice"><%= notice %></p>

<h1>My To Do Lists</h1><br/> <br/> 

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th><br/> <br/> 
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo_lists.each do |todo_list| %>
      <tr>
        <td><%= link_to todo_list.title, todo_list %></td>
        <td><%= todo_list.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><br/> <br/> 

<%= link_to 'New Todo List', new_todo_list_path %></br>
<%= link_to "Return to Home Page", root_path, class: 'link' %> 
